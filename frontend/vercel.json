{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "REACT_APP_API_URL": "https://influncerhub.onrender.com"
    }
  },
  "github": {
    "silent": true
  },
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build"
}