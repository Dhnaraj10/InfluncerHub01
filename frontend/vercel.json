{
  "version": 2,
  "build": {
    "env": {
      "CI": "false",
      "REACT_APP_API_URL": "https://influncerhub.onrender.com"
    }
  },
  "github": {
    "silent": true
  },
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/index.html",
      "status": 200
    }
  ]
}